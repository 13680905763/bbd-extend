{
    "name": "bbd-WeDian快递回填",
    "description": "1.0",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "icons/icon16.png",
        "32": "icons/icon32.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "scripting",
        "tabs",
        "storage"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://weidian.com/user/order-new/searchList?key*"
            ],
            "js": [
                "order.js"
            ],
            "run_at": "document_end"
        }
    ]
}